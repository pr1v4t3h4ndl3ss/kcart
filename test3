repeat task.wait() until game:IsLoaded()

local api_key = _G.API_KEY
if not api_key or api_key == "" then
    return
end

local vps = _G.VPS or ""

local G = {
    [3978370137] = "gpo",
    [7465136166] = "gpo",
}

local I = {
    ["gpo"] = "https://raw.githubusercontent.com/pr1v4t3h4ndl3ss/kcart/refs/heads/main/test",
}

local W = G[game.PlaceId]
if not W then return end

local url = I[W]
if not url then return end

for _ = 1, 5 do
    local U, err = pcall(function()
        loadstring(game:HttpGet(url))()
    end)
    if U then break end
    task.wait(1)
end
